{% extends 'main/main.html.twig' %}
{% block additionalcss %}
    <link rel="stylesheet" href="{{ asset('css/change_user.css') }}" />
{% endblock %}
{% block content %}
<div class="content" id="articles">
    {{ form_start(form) }}
        <h3>{{ 'you_logged_in_as' | trans }}{{ app.user.email }}</h3>
        {{ form_widget(form) }}
        <button type="submit">{{ 'change' | trans }}</button>
    {{ form_end(form) }}
</div>
{% endblock %}